<head>
    <meta charset="utf-8">
    <title>{{ title }}</title>
    {{#each scripts}}
        <script src={{this.script}}></script>
    {{/each}}
    {{#if style}}
        <link href={{style}} rel="stylesheet">
    {{else}}
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/css/materialize.min.css">
    {{/if}}
</head>